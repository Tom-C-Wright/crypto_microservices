# Access key ID required to authenticate deploying microservice to AWS
export AWS_ACCESS_KEY_ID=

# Secret access key required to authenticate deploying microservice to AWS
export AWS_SECRET_ACCESS_KEY=

# API key for authenticating requests to resources under APIGW.
export APIGATEWAY_API_KEY=""

# Stage ID for APIGateway used by service.
export AWS_APIGW_ID=""

# Stage name for APIGateway used by service.
export AWS_APIGW_STAGE=""

# RDS database credentials
export RDS_HOSTNAME=""
export RDS_PORT=""
export RDS_USERNAME=""
export RDS_PASSWORD=""

# Database name for accessing event logs.
export EVENT_LOG_DB_NAME=""